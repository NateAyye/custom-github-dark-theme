        "fontStyle": "bold",
        "foreground": "#8ddb8c"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#6cb6ff"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#6cb6ff"
      }
